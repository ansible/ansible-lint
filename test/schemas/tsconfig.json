{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "lib": ["ESNext"],
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "../../.tox/out",
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": true,
    "stripInternal": true,
    "target": "ESNext",
  },
  "exclude": ["node_modules"],
  "include": ["src/**/*"],
}
